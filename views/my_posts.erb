<h1>My Posts</h1>

<% my_posts = User.find_by(id: session[:user_id])%>

<%# <% my_posts = User.find_by(user_id: current_user.id)%> 
<% personalposts = my_posts.posts%>
<% personalposts.reverse %>

<% personalposts.each do |post| %>

 <div class="row">
    <div class="col s12 m6">
      <div class="card blue-grey darken-1">
        <div class="card-content white-text">
          <span class="card-title"><%= post.title %></span>
          <p><%= post.content %></p>
        </div>
        <div class="card-action">
          <a href="#"><%= post.id %></a>
          <a href="#"><%= post.user.username %></a>
        </div>
      </div>
    </div>
  </div>
<% end %>